<h1>Advanced Search</h1>

<%= form_with model: @search do |f| %>
    <%= f.label :keywords %>
    <%= f.text_field :keywords %>

    <%= f.label :min_price %>
    <%= f.text_field :min_price %>
    
    <%= f.label :max_price %>
    <%= f.text_field :max_price %>

    <%= f.submit "Search" %>
<% end %>

<p><%= link_to "Back", '/coffees' %></p>